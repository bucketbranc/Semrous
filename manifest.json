{
    "manifest_version": 3,
    "name": "Semrous",
    "description": "Semrous Level Extension",
    "version": "0.2.5",
    "background": {
        "service_worker": "service-worker.js"
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["js/Xero.js"]
      }
    ],
    "commands": {
        "history-flooder": {
            "suggested_key": {
              "default": "Alt+Y",
              "mac": "Alt+Y"
            },
            "description": "Flood history."
          },
          "_execute_action": {
            "suggested_key": {
              "windows": "Alt+Y",
              "chromeos": "Alt+Y"
            }
          }
    },
    "action": {
      "default_popup": "popup.html",
      "default_icon": "images/icon.png"
    },
    "permissions": [
        "tabs"
    ]
  }